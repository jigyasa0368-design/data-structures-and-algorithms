// Problem: Josephus problem
// Platform: GFG
// Time Complexity: O(n)
// Space Complexity: O(1)

class Solution {
  public:
    int josephus(int n, int k) {
        // code here
        int ans=0;
        for(int i=2;i<=n;i++){
            ans=(ans+k)%i;
            
        }
       return ans+1;
    }
};
