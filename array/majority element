// Problem: majority element
// Platform: leetcode
// Time Complexity: O(n)
// Space Complexity: O(1)

class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int el=nums[0];
        int cnt=0;
        for(int i=0;i<nums.size();i++){
            if(el==nums[i]) cnt++;
            else cnt--;

            if(cnt==0){
                el=nums[i+1];
            }
            
        }
        return el;
        
    }
};
