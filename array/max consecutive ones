// Problem: max consecutive ones
// Platform: leetcode
// Time Complexity: O(n)
// Space Complexity: O(1)

class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int cnt=0;
        int maxi=INT_MIN;
        for(int i=0;i<nums.size();i++){
            if(nums[i]!=1) cnt=0;
            else cnt++;

            maxi=max(cnt,maxi);
        }
        return maxi;
    }
};
