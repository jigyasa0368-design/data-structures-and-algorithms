// Problem: delete node in a LL(solve without using head)
// Platform: Leetcode
// Approach: sliding window
// Time Complexity: O(1)
// Space Complexity: O(1)


class Solution {
public:
    void deleteNode(ListNode* node) {
        
        
           node->val=node->next->val;
           ListNode* temp=node->next;
           node->next=node->next->next;
           delete temp;    
        
    }
};
