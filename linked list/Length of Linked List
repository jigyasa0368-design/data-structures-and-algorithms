// Problem: Length of Linked List
// Platform: GFG
// Time Complexity: O(n)
// Space Complexity: O(1)


class Solution {
  public:
    int getCount(Node* head) {
        // Code here
        int cnt=0;
        Node* temp=head;
        while(temp != NULL){
            cnt++;
            temp=temp->next;
            
            
        }
        return cnt;
    }
};

